{
	"version": "2.0.0",
	"tasks": [
		// ========================================
		// AUTO-START ON FOLDER OPEN
		// ========================================
		{
			"label": "SLATEPI: Auto-Start Dashboard",
			"type": "shell",
			"command": "${workspaceFolder}/.venv/Scripts/python.exe",
			"args": ["${workspaceFolder}/agents/aurora_dashboard_server.py"],
			"isBackground": true,
			"problemMatcher": {
				"pattern": {
					"regexp": "^$"
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": ".",
					"endsPattern": "Uvicorn running|Application startup complete|SLATEPI Dashboard"
				}
			},
			"presentation": {
				"reveal": "silent",
				"panel": "dedicated",
				"showReuseMessage": false
			},
			"runOptions": {
				"runOn": "folderOpen"
			}
		},

